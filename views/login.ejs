<!DOCTYPE html>
<html lang="en">
<head>
  <title>Bootstrap Case</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link rel="stylesheet" href="stylesheets/style.css">
  <script
  src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
  <script src="angularjs/index.js"></script>
   <script src="angularjs/session.js"></script>
  <script src="angularjs/login.js"></script>

</head>

<body ng-app="mainApp" ng-controller="loginController" >
<div class="container">
<div class="row">
<div class="col-md-12" style="height:50px;float:centre;">
 <!-- <a href="/"><img src="img/logo1.png"></a> -->
</div>
</div>

<div class="row">

  <div class="col-md-4">
   
  </div>
    <div class="col-md-4">
    <a href="/"><img src="/img/logo.png" height="35%" width="45%" style=" display:block;margin-left: auto;
    margin-right: auto"></a>
<br>
<br>

      <ul class="nav nav-tabs">
      <li><a href="#" ng-click="selectedSigninTab();"><span class="sp spr ar"></span>SignIn</a></li>
      <li><a href="#" ng-click="selectedRegisterTab();"><span class="sp spr pl"></span>Register</a></li>
      </ul>
      <br>
  
      <div class="sign-in-fields" ng-hide="selectedSignIn">
        <div class="input-group">
          <span class="input-group-addon" id="basic-addon1">@</span>
          <input type="text" class="form-control" ng-model="username" placeholder="Username" aria-describedby="basic-addon1">
        </div>
        <br>
        <div class="input-group">
          <span class="input-group-addon" id="basic-addon1">*</span>
          <input type="password" class="form-control" ng-model="password" placeholder="Password" aria-describedby="basic-addon1">
        </div>
        <br>
         <!-- <div class="input-group"> -->
         <button type="button" ng-click="authenticate();" class="btn btn-primary btn-block" name="submit">Sign In</button>
         <!-- </div> -->
      </div>

      <br>
      <div class="registration-fields" ng-hide="selectedRegister">

    
        <div class="input-group">
        <span class="input-group-addon" id="basic-addon1"></span>
          <input type="text" class="form-control" ng-model="email" placeholder="email" aria-describedby="basic-addon1" required>
        </div>
        <br>
        <div class="input-group">
        <span class="input-group-addon" id="basic-addon1"></span>
          <input type="text" class="form-control" ng-model="reenteredemail" placeholder="re-enter email" aria-describedby="basic-addon1" required>
        </div>
        <br>
        <div class="input-group">
        <span class="input-group-addon" id="basic-addon1"></span>
          <input type="password" class="form-control" ng-model="password" placeholder="password" aria-describedby="basic-addon1" required>
        </div>
        <br>
   
        <div class="input-group">
        <span class="input-group-addon" id="basic-addon1"></span>
          <input type="text" class="form-control" ng-model="firstName" placeholder="First Name" aria-describedby="basic-addon1" required>
            </div>
            <br>
         <div class="input-group">
         <span class="input-group-addon" id="basic-addon1"></span>
            <input type="text" class="form-control" ng-model="lastName" placeholder="Last Name" aria-describedby="basic-addon1" required>
            </div>
             <br>
        <!--  <div class="input-group"> -->
         <button type="button" ng-click="register();" class="btn btn-primary btn-block" name="submit">Register</button>
       <!--   </div> -->

      </div>

    
    
  </div>
  <div class="col-md-4">
  <h2></h2>
  </div>
</div>
</div>
</body>

</html>